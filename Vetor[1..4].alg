algoritmo "semnome"

var

 v : vetor[1..4] de inteiro
 i : inteiro

inicio

      //v[1] := 7
      //v[2] := 12
      //v[3] := 20
      Para i := 1 ate 4 faca
           EscrevaL("V(", i, ")= ")
           Leia(v[i])
      FimPara
      
      Para i := 1 ate 4 faca
           EscrevaL("V(", i, ")= ",v[i])
      FimPara

fimalgoritmo
