
algoritmo "ex006"


var

    n1, ant, suc : Inteiro


inicio

      EscrevaL("Informe um numero :")
      leia(n1)
      
      ant <- n1 - 1
      suc <- n1 + 1
      
      EscrevaL("O antecessor de ", n1, " é ", n1-1)
      EscrevaL("o sucessor de ", n1, " é ", n1+1)


fimalgoritmo

